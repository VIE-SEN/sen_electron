{
  "name": "sen",
  "version": "1.1.1",
  "description": "sendesc",
  "main": "main.js",
  "scripts": {
    "dev": "electron .",
    "build": "electron-builder build"
  },
  "repository": "",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "sen",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^15.5.5",
    "electron-builder": "^21.2.0"
  },
  "build": {
    "productName": "sen",
    "appId": "com.sen.test",
    "copyright": "seb",
    "directories": {
      "output": "build"
    },
    "win": {
      "icon": "",
      "target": [
        {
          "target": "nsis"
        }
      ]
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://127.0.0.1:5500/"
      }
    ],
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "",
      "uninstallerIcon": "",
      "installerHeaderIcon": "",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "sen"
    }
  },
  "dependencies": {
    "electron-updater": "^4.1.2"
  }
}
